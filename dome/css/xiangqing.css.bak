*{
	margin: 0;
	padding: 0;
}
.xiangqing_content{
	width: 1200px;
	margin: auto;
}
.xiangqing_content1{
	padding: 0 20px;
	overflow: hidden;
}
.xiangqing_content_top{
	position: relative;
	width: 940px;
	float: left;
	background: white;
	padding: 20px;
	overflow: hidden;
}
.FDJ{
	position: relative;
	float: left;
	width: 400px;
	padding: 15px;
	height: 500px;
}
.xiangqing_content_right{
	width: 490px;
	float: right;
	margin-top: 12px;
}
.xiangqing_content_right ul li{
	overflow: hidden;
	font-size: 16px;
}
.xiangqing_content_right ul li p{
	float: left;
	margin-top: 10px;
}
#Prices,#price{
	margin-top: 10px;
	font-size: 20px;
	color: #aa2828;
}


#fudu{
	background: #aa2828;
	color: white;
	padding: 3px 8px;
	font-size: 12px;
	margin-left: 20px;
}
.skli2 p:last-child{
	margin-left: 20px;
}
.skli3{
	padding-bottom: 8px;
}
.skli3 p:first-child, .skli4 p:first-child, .skli5 p:first-child,.skli6 p:nth-child(2n-1){
	min-width: 40px;
	text-align: right;
}
#rutu{
	font-weight: bold;
	color: #5e5e5e;
	border: 2px solid firebrick;
	margin-right: 20px;
	padding: 5px;
	min-width: 40px;
	line-height: 20px;
	border-radius: 4px;
	cursor: not-allowed;
	line-height: 40px;
}
.hover1:hover,.hover2:hover{
	font-weight: bold;
	color: #5e5e5e;
	border: 2px solid firebrick;
}
.hover1,.hover2{
	cursor: pointer !important;
}
.fou{
	border: 1px solid gainsboro !important;
	color: darkgray !important;
}
.fou1{
	border: 1px solid gainsboro !important;
	color: darkgray !important;
}
.xiangqing_Color,.xiangqing_Size{
	border: 1px solid gray;
	margin-right: 20px;
	padding: 5px;
	min-width: 40px;
	line-height: 20px;
	border-radius: 4px;
	color: dimgrey;
	cursor: not-allowed;
	line-height: 40px;
}
.xiangqing_size{
	color: #5e5e5e;
	font-weight: bold;
	border: 2px solid firebrick;
}
.xiangqing_color{
	color: #5e5e5e;
	font-weight: bold;
	border: 2px solid firebrick;
}
.skli3 p:first-child, .skli4 p:first-child, .skli5 p:first-child{
	margin-right: 20px;
	font-size: 16px;
	color: darkgray;
	line-height: 40px;
}
.skli5 p:nth-child(2){
	border: 1px solid gainsboro;
	border-radius: 3px;
	margin-top: 18px;
	cursor:pointer;
}
.skli5 p:nth-child(2) span{
	display: inline-block;
	font-weight: bold;
	width: 30px;
	height: 25px;
	text-align: center;
	font-size: 16px;
	line-height: 25px;
}
.skli5 p:nth-child(3){
	margin-top: 18px;
	color: gainsboro;
}
.skli6{
	background: #fafafa;
	border: 1px solid gainsboro;
	width: 380px;
	font-size: 16px;
	margin-top: 30px;
	padding: 10px;
}
.skli6 p:nth-child(2n){
	width: 280px;
	margin-left: 10px; 
}
.skli7{
	border-bottom: 1px dashed gainsboro;
}
.skli7 button{
	width: 130px;
	height: 45px;
	border-radius: 3px;
	border: 1px solid #44aaf6;
	font-size: 18px;
	color: #44aaf6;
	background: white;
	margin: 20px 20px 5px 0;
}
#details{
	text-align: center !important;
}




.xiangqing_content_right1 {
	width: 220px;
	float: right;
	border: 1px solid gainsboro;
	border-radius: 5px;
	padding: 10px;
	background: white;
	/*border: 1px solid red;*/
}
.xiangqing_content_right1 ul li{
	border-bottom: 1px solid gainsboro;
	padding: 10px;
}
.xiangqing_content_right1 ul li:last-child{
	border: none;
}
.xiangqing_content_right1 ul li img{
	width: 100%;
}
.xiangqing_content_right1 ul li span{
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp:1;
	-webkit-box-orient: vertical;
	font-size: 12px;
	line-height: 20px;
}
.xiangqing_content_right1 ul li p{
	color: #aa2828;
	font-size: 16px;
	margin: 10px 0 0 0;
}

.xiangqing_content_rleft1{
	width: 940px;
	float: left;
	overflow: hidden;
	margin-top: 30px;
	color: darkgray;
	background: white;
}
.xiangqing_top{
	overflow: hidden;
	border-bottom: 1px solid gainsboro;
}
.nav-tabs{
	height: 50px;
	margin-left: -10px;
	color: black;
	font-weight: bold;
	font-size: 20px;
}
.nav-tabs li a{
	height: 50px;
	padding: 10px 20px;
	color: black;
}
#home,#profile,#messages{
	padding: 20px;
	border: 1px solid gainsboro;
	border-top: none;
}
#home ul li{
	width: 25%;
	float: left;
	
}
.xiangqing_picture{
	text-align: center;
}
.clic_nav{
	cursor: pointer;
}


































.xiangqing_section{
	position: relative;
	top: 0;
	left: 0;
	width: 1200px;
	margin: auto;
	height: 330px;
	background: white;
	border: 1px solid gainsboro;
	margin-bottom: 40px;
	border-radius: 5px;
}
.next{
	position: absolute;
	left: 15px;
	top: 120px;
	height: 50px;
}
.last{
	position: absolute;
	right: 15px;
	height: 50px;
	top: 120px;
}
.section_ul{
	width: 1080px;
	height: 250px;
	position: absolute;
	left: 60px;
	top: 50px;
	overflow: hidden;
}
.sect_ul{
	text-align: center;
	position: absolute;
	left: 0;
	top: 0;
}
.sect_ul li{
	width: 200px;
	height: 200px;
	margin-right: 20px;
	float: left;
	border: 1px solid gainsboro;
}
.sect_ul li:hover{
	border: 1px solid #44aaf6;
	-webkit-animation: pulse 0.5s linear;
	-moz-animation: pulse 0.5s linear;
	-ms-animation: pulse 0.5s linear;
	-o-animation: pulse 0.5s linear;
	animation: pulse 0.5s linear;
}
.sect_ul li img{
	width: 100%;
	height: 100%;
}
.sect_ul li p{
	margin-top: 5px;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp:1;
	-webkit-box-orient: vertical;
}
.sect_ul li span{
	color: #aa2828;
	font-weight: bold;
}
.LJGMbtn{
	background: #44aaf6 !important;
	color: white !important;
	font-weight: bold;
}
