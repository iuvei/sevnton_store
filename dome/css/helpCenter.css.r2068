.helpCent_wrap{
    width: 1200px;
    margin: auto;
    margin-bottom: 80px;
}
.helpCent_left{
    background: white;
    float: left;
    width: 16%;
    border: 1px solid gainsboro;
    border-top: none;
}
.helpCent_type1{
    margin-bottom: 10px;
}
.helpCent_type .helpCent_type1:last-child{
    margin-bottom: 0;
}
.helpCent_type2,.helpCent_type3{
    display: none;
}
.type1_con{
    line-height: 40px;
    font-size: 16px;
    color: black;
    border-bottom: 1px solid gainsboro;
    border-top: 1px solid gainsboro;
    padding: 0 12%;
    cursor: pointer;
    background: #fafafa;
}
.type2_con a{
    font-size: 14px;
    color: grey;
    margin-left: 15%;
    cursor: pointer;
    line-height: 30px;
}
.type3_con a{
    font-size: 12px;
    color: grey;
    margin-left: 20%;
    cursor: pointer;
}
.helpCent_active{
    color: #3da8f9 !important
}

.helpCent_right{
    font-family: "微软雅黑"!important;
    float: right;
    width: 83%;
    border: 1px solid gainsboro;
    min-height: 800px;
    background: white;
    padding: 10px;
}
.helpCent_right img{
    max-width: 100% !important;
}